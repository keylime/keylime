{
    "version": "2.4",
    "type": "update",
    "components": {
        "agent": {
            "add": {
                "api_versions": "default"
            }
        },
        "registrar": {
            "add" : {
                "malformed_cert_action": "warn"
            }
        },
        "logging": {
            "replace" : {
                "format": {
                    "section": "formatter_formatter",
                    "option": "format",
                    "default": "%(asctime)s.%(msecs)03d - %(name)s - %(levelname)s - %(message)s %(reqidf)s"
                }
            }
        }
    }
}
